-family: $fontMain;\r\n      font-weight: 600;\r\n   }\r\n   .dropdownContent a {\r\n      color: white;\r\n      display: block;\r\n      padding: 15px 16px;\r\n      text-align: center;\r\n      min-width: 180px;\r\n      border: 1px solid black;\r\n   }\r\n   .dropdownContent a:hover {\r\n      background-color: #2E2E2E;\r\n   }\r\n   #guidesDropdown:hover .dropdownContent {\r\n      display: block;\r\n   }\r\n   // *Drop Down End* //\r\n   #discordHeader {\r\n      display: flex;\r\n      width: 307px;\r\n      flex-flow: column nowrap;\r\n      justify-content: center;\r\n      align-items: center;\r\n      p {\r\n         margin-top: 5px;\r\n         color: $accentBlue;\r\n         font-size: 16px;\r\n         font-weight: 600;\r\n      }\r\n      #discordButton {\r\n         width: 170px;\r\n         height: 30px;\r\n         border-radius: 40px;\r\n         border: px solid black;\r\n         background-color: #1b1b1b;\r\n         text-align: center;\r\n         cursor: pointer;\r\n      }\r\n      #discordButton p {\r\n         color: white;\r\n      }\r\n      #discordButton:hover {\r\n         background-color: #2b2b2b;\r\n      }\r\n   }\r\n   #acherusTitle {\r\n      padding: 0px 50px 0px 0px;\r\n   }\r\n   a {\r\n      text-decoration: none;\r\n   }\r\n}\r\n\r\n/// Content ///\r\n// !Global! //\r\n.main {\r\n   width: 100%;\r\n   display: flex;\r\n   flex-flow: column nowrap;\r\n   border: 3px solid black;\r\n   align-items: center;\r\n   letter-spacing: 0.01%;\r\n   .contentFrame {\r\n      width: 1520px;\r\n      display: flex;\r\n      flex-flow: column nowrap;\r\n      align-items: center;\r\n      background-color: $backdrop;\r\n      text-align: center;\r\n      // ***Guide Nav*** //\r\n      .guideNav {\r\n         position: fixed;\r\n         left: 74%;\r\n         width: 220px;\r\n         border-left: 3px solid $accentBlue;\r\n         padding-left: 10px;\r\n         li {\r\n            line-height: 25px !important;\r\n            font-weight: 900;\r\n         }\r\n         a {\r\n            text-decoration: none;\r\n            color: white;\r\n            padding-left: 3px;\r\n\r\n         }\r\n      }\r\n   }\r\n}\r\n\r\n// !Homepage! //\r\n#homeMain #homeContent {\r\n   display: flex;\r\n   flex-flow: column nowrap;\r\n   align-items: center;\r\n   img {\r\n      margin-top: 30px;\r\n   }\r\n   h1 {\r\n      color: $dkRed;\r\n      padding: 20px 0px 0px 0px;\r\n   }\r\n   p {\r\n      width: 1045px;\r\n      padding: 20px 0px 20px 0px;\r\n      font-size: 16px;\r\n   }\r\n   a {\r\n      text-decoration: none;\r\n      color: $accentBlue;\r\n      font-weight: 600;\r\n      font-size: 17px;\r\n   }\r\n}\r\n\r\n// !Unholy Guide! //\r\n#unhGuideContent {\r\n   position: relative;\r\n   right: 13%;\r\n   text-align: left;\r\n   padding: 15% 10% 0% 10%;\r\n   z-index: 0;\r\n   h1 {\r\n      padding-bottom: 20px;\r\n   }\r\n   h2 {\r\n      padding-bottom: 20px;\r\n   }\r\n   h3 {\r\n      margin-left: 4%;\r\n   }\r\n   p {\r\n      margin-left: 2%;\r\n      padding-bottom: 20px;\r\n      font-weight: 600;\r\n      width: 850px;\r\n      line-height: 25px;\r\n   }\r\n   li {\r\n      list-style: none;\r\n      margin-left: 5%;\r\n      line-height: 34px;\r\n      font-size: 12.8px;\r\n   }\r\n   .strTick {\r\n      color: #00FF19;\r\n   }\r\n   .weakTick {\r\n      color: #FF0000;\r\n   }\r\n   #unhTalents {\r\n      margin-top: 20px;\r\n      .unhTalentTierImg {\r\n         padding-bottom: 10px;\r\n         margin-left: 1.2%;\r\n         width: 90%;\r\n      }\r\n      .unhTalentTierDesc {\r\n         margin-bottom: 4%;\r\n      }\r\n   }\r\n   // ***Talent Box*** //\r\n   #talentChart {\r\n      margin-top: 10px;\r\n      margin-left: 2%;\r\n      margin-bottom: 30px;\r\n      display: grid;\r\n      width: 750px;\r\n      grid-template-rows: repeat(3, 170px);\r\n      font-weight: 700;\r\n      .talentChartRow {\r\n         border: 0.4px solid #9F9F9F;\r\n         display: grid;\r\n         grid-auto-flow: row;\r\n         grid-template-columns: 300px 80px 250px;\r\n         line-height: 19px;\r\n         align-items: center;\r\n         .talentTiers {\r\n            text-align: right;\r\n         }\r\n         .talentNames {\r\n            color: $dkRed;\r\n         }\r\n         li {\r\n            line-height: 19px;\r\n            font-size: 12.8px;\r\n         }\r\n         h4 {\r\n            text-align: center;\r\n            font-size: 14px;\r\n         }\r\n         a {\r\n            padding-left: 3px;\r\n         }\r\n \r\n      }        \r\n      #soloContent, #raidContent {\r\n         background-color: #161616;\r\n      }\r\n      #cleaveContent {\r\n         background-color: #222222;\r\n      }\r\n   }\r\n}\r\n","style\\_variables.scss":"// Colors\r\n$dkRed: #C41F3B;\r\n$bloodRed: #FF4700;\r\n$frostBlue: #67BCFF;\r\n$unholyGreen: #40BC40;\r\n$accentBlue: #618DA6;\r\n$backdrop: #1B1B1B;\r\n\r\n// Fonts\r\n$fontMain: 'Raleway', ;\r\n$fontTitle: 'Metal Mania', ;\r\n\r\n// Testing\r\n$testBorder1: 5px dashed #59E259;\r\n$testBorder2: 5px dashed red;"},"lineCount":null}},"error":null,"hash":"f4c8d7a1d2714fb4ee427d042158447f","cacheData":{"env":{}}}